{
    "name": "verbb/doxter",
    "description": "Slick Markdown Editor and Smart Text Parser for Craft CMS",
    "type": "craft-plugin",
    "version": "5.0.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "doxter"
