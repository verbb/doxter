        "docs": "https://github.com/verbb/doxter",
        "rss": "https://github.com/verbb/doxter/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        },
        {
            "name": "Selvin Ortiz",
            "email": "selvin@selvin.co"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "verbb/base": "^2.0.0",
        "michelf/php-smartypants": "^1.8",
        "mundschenk-at/php-typography": "^6.3",
        "spatie/yaml-front-matter": "^2.0"
